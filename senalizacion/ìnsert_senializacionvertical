-- Inserts para llenar las tablas desde la base vieja a la nueva


-- Mojon Kilométrico  I10 , id_elem 280

INSERT INTO senializacion.senializacionvertical (
  id_tramo,
  id_banda,
  id_senal,
  forma,
  dimensiones,
  soporte,
  id_soporte,
  pk,
  contenido,
  imagen,
  leyenda_extra,
  fmodif
)
SELECT
  vs.id_tramo,
  vs.id_banda,
  142 AS id_senal,
  vs.forma,
  vs.dimensiones,
  vs.soporte,
  vs.id_soporte,
  vs.pk,
  vs.contenido,
  vs.iconografia,
  vs.obs,
  fmodif
FROM
  gisdata.v_senializacionvertical AS vs
WHERE
  vs.id_elem = 280;



